{
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://educrit-api.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://educrit-api.onrender.com/auth/:path*"
    },
    {
      "source": "/",
      "destination": "https://educrit-api.onrender.com/"
    },
    {
      "source": "/items/:id",
      "destination": "https://educrit-api.onrender.com/items/:id"
    }
  ]
}